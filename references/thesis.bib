journal = {Pac Symp Biocomput},
keywords = {genetic interaction,genetic network,pleiotropy},
number = {Ld},
pages = {212--223},
pmid = {24297548},
title = {{Dissection of complex gene expression using the combined analysis of pleiotropy and epistasis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24297548},
volume = {19},
year = {2014}
}
