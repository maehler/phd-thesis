@article{Istrail2007,
abstract = {The definitive feature of the many thousand cis-regulatory control modules in an animal genome is their information processing capability. These modules are "wired" together in large networks that control major processes such as development; they constitute "genomic computers." Each control module receives multiple inputs in the form of the incident transcription factors which bind to them. The functions they execute upon these inputs can be reduced to basic AND, OR and NOT logic functions, which are also the unit logic functions of electronic computers. Here we consider the operating principles of the genomic computer, the product of evolution, in comparison to those of electronic computers. For example, in the genomic computer intra-machine communication occurs by means of diffusion (of transcription factors), while in electronic computers it occurs by electron transit along pre-organized wires. There follow fundamental differences in design principle in respect to the meaning of time, speed, multiplicity of processors, memory, robustness of computation and hardware and software. The genomic computer controls spatial gene expression in the development of the body plan, and its appearance in remote evolutionary time must be considered to have been a founding requirement for animal grade life. ?? 2007 Elsevier Inc. All rights reserved.},
author = {Istrail, Sorin and De-Leon, Smadar Ben Tabou and Davidson, Eric H.},
doi = {10.1016/j.ydbio.2007.08.009},
file = {:Users/niklasm/Dropbox/mendeley{\_}docs/Developmental Biology/2007/Istrail, De-Leon, Davidson - The regulatory genome and the computer.pdf:pdf},
isbn = {00121606},
issn = {00121606},
journal = {Developmental Biology},
keywords = {Biological logic computations,Gene regulatory networks,Regulatory information processing,regulatory genome},
mendeley-tags = {regulatory genome},
number = {2},
pages = {187--195},
pmid = {17822690},
title = {{The regulatory genome and the computer}},
volume = {310},
year = {2007}
