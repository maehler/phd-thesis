@article{Doig2013,
abstract = {BACKGROUND: Biopsies taken from individual tumours exhibit extensive differences in their cellular composition due to the inherent heterogeneity of cancers and vagaries of sample collection. As a result genes expressed in specific cell types, or associated with certain biological processes are detected at widely variable levels across samples in transcriptomic analyses. This heterogeneity also means that the level of expression of genes expressed specifically in a given cell type or process, will vary in line with the number of those cells within samples or activity of the pathway, and will therefore be correlated in their expression.

RESULTS: Using a novel 3D network-based approach we have analysed six large human cancer microarray datasets derived from more than 1,000 individuals. Based upon this analysis, and without needing to isolate the individual cells, we have defined a broad spectrum of cell-type and pathway-specific gene signatures present in cancer expression data which were also found to be largely conserved in a number of independent datasets.

CONCLUSIONS: The conserved signature of the tumour-associated macrophage is shown to be largely-independent of tumour cell type. All stromal cell signatures have some degree of correlation with each other, since they must all be inversely correlated with the tumour component. However, viewed in the context of established tumours, the interactions between stromal components appear to be multifactorial given the level of one component e.g. vasculature, does not correlate tightly with another, such as the macrophage.},
author = {Doig, Tamasin N and Hume, David a and Theocharidis, Thanasis and Goodlad, John R and Gregory, Christopher D and Freeman, Tom C},
doi = {10.1186/1471-2164-14-469},
file = {:Users/niklasm/Dropbox/mendeley{\_}docs/BMC genomics/2013/Doig et al. - Coexpression analysis of large cancer datasets provides insight into the cellular phenotypes of the tumour microenvironmen.pdf:pdf},
issn = {1471-2164},
journal = {BMC genomics},
keywords = {Computer Graphics,Conserved Sequence,Conserved Sequence: genetics,Databases, Genetic,Gene Expression Profiling,Genes, Neoplasm,Genes, Neoplasm: genetics,Humans,Neoplasms,Neoplasms: genetics,Neoplasms: pathology,Phenotype,Transcription, Genetic,Transcription, Genetic: genetics,Tumor Microenvironment,Tumor Microenvironment: genetics},
month = {jan},
number = {1},
