journal = {Molecular biology and evolution},
keywords = {Animals,Brain,Brain Chemistry,Brain: metabolism,Cluster Analysis,Female,Fish Proteins,Fish Proteins: genetics,Fish Proteins: metabolism,Gene Expression Profiling,Gene Regulatory Networks,Genetic Speciation,Male,Muscles,Muscles: chemistry,Muscles: metabolism,Oligonucleotide Array Sequence Analysis,Phenotype,Principal Component Analysis,Salmonidae,Salmonidae: classification,Salmonidae: genetics,Salmonidae: metabolism},
