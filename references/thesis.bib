file = {:Users/niklasm/Dropbox/mendeley{\_}docs/Proceedings of the National Academy of Sciences of the United States of America/2015/Huang et al. - Genetic basis of transcriptome diversity in Drosophila melanogaster.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {epistasis,genome-wide association,mean eQTLs,novel transcribed regions,variance eQTLs},
number = {44},
pages = {E6010--9},
pmid = {26483487},
title = {{Genetic basis of transcriptome diversity in Drosophila melanogaster.}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1519159112$\backslash$nhttp://www.ncbi.nlm.nih.gov/pubmed/26483487$\backslash$nhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4640795},
